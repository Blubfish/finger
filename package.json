    "@tamagui/config": "^1.131.1",
    "@tamagui/core": "^1.131.1",
    "expo": "53.0.19",
    "expo-constants": "17.1.7",
    "expo-font": "13.3.2",
    "expo-router": "5.1.3",
    "expo-splash-screen": "0.30.10",
